@model CMS_Solution.WebRuby.ViewModel.FooterViewModel
@using Core.Common.Utilities
@using Business.Entities.Domain
@using CMS_Solution.WebRuby.Common;
@{
    UserManager user = new UserManager();
    if (Session[AppSetting.SessionUser] != null)
    {

        user = (UserManager)Session[AppSetting.SessionUser];
    }
}

@if (Model.footerTopMap != null)
{
    @Html.Raw((Model.footerTopMap != null ? (Model.footerTopMap.StaticContent.Replace("&amp;", "&")
                                 .Replace("&lt;", "<")
                                 .Replace("&gt;", ">")
                                 .Replace("&quot;", "\"")
                                 .Replace("&#039;", "\'") ?? "") : ""))
}

@Html.Partial("_QuickMenuView", Model)