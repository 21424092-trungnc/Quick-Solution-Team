@using Core.Common.UI;
@using CMS_Solution.WebRuby.Common;
@model  CMS_Solution.WebRuby.ViewModel.AboutViewModel
@{
    Layout = "~/Views/Shared/_ColumnsOne.cshtml";
    Html.AddTitleParts("Giới thiệu-Quick Solution Team");
}

<section class="background-about mt-2">
    <section class="bg-about">
        <h1>Giới thiệu</h1>
    </section>
</section>
<section class="container mg-crumb">
    <ul class="breadcrumb">
        <li class="breadcrumb-item"><a href="@Url.RouteUrl("HomePage")">Trang chủ</a></li>
        <li class="breadcrumb-item active">Giới thiệu</li>
    </ul>
</section>
<section class="office">
    <section class="container">
        <h2 class="title-about">
            @Html.Raw((Model.staticContentMap != null ? (Model.staticContentMap.StaticTitle.Replace("&amp;", "&")
                     .Replace("&lt;", "<")
                     .Replace("&gt;", ">")
                     .Replace("&quot;", "\"")
                     .Replace("&#039;", "\'") ?? "") : "")).
    </h2>
    @Html.Raw((Model.staticContentMap != null ? (Model.staticContentMap.StaticContent ?? "") : ""))
</section>
</section>